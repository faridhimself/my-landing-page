{
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/blog",
      "rewrite": "/blog/"
    },
    {
      "route": "/projects",
      "rewrite": "/projects/"
    },
    {
      "route": "/contact",
      "rewrite": "/contact/"
    },
    {
      "route": "/services",
      "rewrite": "/services/"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
